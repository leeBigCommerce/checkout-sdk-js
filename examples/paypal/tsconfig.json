{
  "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "importHelpers": true,
      "lib": [
          "dom",
          "dom.iterable",
          "es6",
          "scripthost"
      ],
      "module": "es6",
      "moduleResolution": "node",
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "strict": true,
      "stripInternal": true,
      "target": "es5",
      "types": ["cypress", "node", "grecaptcha"]
  },
  "include": [
      "app/**/*.ts"
  ]
}
